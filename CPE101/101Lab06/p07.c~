char letter(int r, int c)
{
   if(r < 11)
   {
      if((c-1) - r < 0)
      {
         return 'C';
      }
      else
      { 
         return 'X';
      }
   }
   else
   {
      if(r > 9)
      {
         if((19 - r - c) < 0)
         {
            return 'X';
         }
         else
         {
            return 'C';
         }
      }
   }
   return 0;
}
