char letter(int r, int c)
{
   if((c < 10) && (c > 3) && (r < 4) && (r > 1))   
   {
      return 'Z';
   }
   else
   {
      return 'T';
   }

   if(( c < 15) && (c > 11) && (r < 16) && (r > 12))
   {
      return 'X';
   }
   else
   {
      return'T';
   }
}
